{
  "buildCommand": "npm run build",
  "outputDirectory": "apps/control-room/.next",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_SYNTHIA_BACKEND_URL": "@synthia_backend_url"
  },
  "regions": ["sfo1"],
  "functions": {
    "apps/control-room/src/app/api/**/*.ts": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "claude/deploy-vercel-backend-update-H6MlJ": true
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
